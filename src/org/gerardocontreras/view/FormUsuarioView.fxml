<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.gerardocontreras.controller.FormUsuarioController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/FormularioUsuarios.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfUser" layoutX="57.0" layoutY="270.0" prefHeight="34.0" prefWidth="334.0" />
      <TextField fx:id="tfPassword" layoutX="57.0" layoutY="375.0" prefHeight="34.0" prefWidth="334.0" />
      <ComboBox fx:id="cmbEmpleado" layoutX="57.0" layoutY="473.0" prefHeight="34.0" prefWidth="334.0" />
      <Button fx:id="btnRegistrar" layoutX="250.0" layoutY="630.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="67.0" prefWidth="124.0" text="Registrar">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Button>
      <ComboBox fx:id="cmbNivelAcceso" layoutX="57.0" layoutY="577.0" prefHeight="34.0" prefWidth="334.0" />
      <Button fx:id="btnEmpleado" layoutX="100.0" layoutY="630.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="67.0" prefWidth="124.0" text="Agregar Empleado" wrapText="true">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
